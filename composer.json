{
    "name": "ahmedghanem00/tempmail-api-client",
    "description": "An API client for Temp-Mail service ( https://rapidapi.com/Privatix/api/temp-mail )",
    "type": "library",
    "license": "MIT",

    "keywords": [
        "api-client",
        "temporary email",
        "disposable email"
    ],

    "authors": [
        {
            "name": "Ahmed Ghanem",
            "email": "ahmedghanem7361@gmail.com",
            "homepage": "https://github.com/ahmedghanem00"
        }
    ],

    "autoload": {
        "psr-4": {
            "ahmedghanem00\\TempMailClient\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "ahmedghanem00\\TempMailClient\\Tests\\": "tests/"
        }
    },

    "require": {
        "php": ">=8.2",
        "symfony/http-client": "6.*",
        "doctrine/collections": "2.*",
        "voku/arrayy": "7.*",
        "phlak/strgen": "dev-master"
    },

    "require-dev": {
        "phpunit/phpunit": "dev-main",
        "phpstan/phpstan": "1.10.x-dev"
    },

    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "phpstan": "@php vendor/bin/phpstan analyse src tests"
    },

    "minimum-stability": "stable"
}
